    <select id="getBeanById" resultMap="TSysJxsModelMap" parameterType="com.wellsys.pioneer.module.tSysJxs.model.TSysJxsModel" >
        SELECT
            id
            ,bh
            ,mc
            ,zxsId
            ,areaId
            ,status

        FROM
            sys_jxxl
        WHERE
            1=1
            AND id=#{id}
