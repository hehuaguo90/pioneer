				  ckbs.each(function(){
				  $(this).parent().parent().remove();
				  });
				 }
			 function validateZgzzdj(){
				 $("input").each(function(i){
			            $(this).removeClass("error");
			        });
				 var showMsg = "";
				 var reg = /^([1-9]\d{0,9}|0)?$/; //正则验证是否为合理整数
				 //alert($("input[name=cngz]").length);
				 var cngzs = $("input[name=cngz]");
				 var hbgzs = $("input[name=hbgz]");
				 var cdgzs = $("input[name=cdgz]");
				 var jyscls = $("input[name=jyscl]");
				 var jyxsls = $("input[name=jyxsl]");
				 var sls = $("input[name=sl]");
				 var i = 0;
				 var j;
				 for(j = 0;j < cngzs.length; j++){
					 if(!reg.test(cngzs[j].value)){
						 i++;
						 var key = $(cngzs[j]).attr("id");
						 $("#" + key).addClass("error");
						   showMsg += '<p class="text-danger" onclick="locationErr(\'' + key + '\', this)">' + i + "、" + "成年公猪" + "，" + "请输入合理正整数！" + "</p>";
					    } 
				 }
				 for(j = 0;j < hbgzs.length; j++){
					 if(!reg.test(hbgzs[j].value)){
						 i++;
						 var key = $(hbgzs[j]).attr("id");
						 $("#" + key).addClass("error");
						   showMsg += '<p class="text-danger" onclick="locationErr(\'' + key + '\', this)">' + i + "、" + "后备公猪" + "，" + "请输入合理正整数！" + "</p>";
					    } 
				 }
				 for(j = 0;j < cdgzs.length; j++){
					 if(!reg.test(cdgzs[j].value)){
						 i++;
						 var key = $(cdgzs[j]).attr("id");
						 $("#" + key).addClass("error");
						   showMsg += '<p class="text-danger" onclick="locationErr(\'' + key + '\', this)">' + i + "、" + "测定公猪" + "，" + "请输入合理正整数！" + "</p>";
					    } 
				 }
				 for(j = 0;j < jyscls.length; j++){
					 if(!reg.test(jyscls[j].value)){
						 i++;
						 var key = $(jyscls[j]).attr("id");
						 $("#" + key).addClass("error");
						   showMsg += '<p class="text-danger" onclick="locationErr(\'' + key + '\', this)">' + i + "、" + "精液生产量" + "，" + "请输入合理正整数！" + "</p>";
					    } 
				 }
				 for(j = 0;j < jyxsls.length; j++){
					 if(!reg.test(jyxsls[j].value)){
						 i++;
						 var key = $(jyxsls[j]).attr("id");
						 $("#" + key).addClass("error");
						   showMsg += '<p class="text-danger" onclick="locationErr(\'' + key + '\', this)">' + i + "、" + "精液销售量" + "，" + "请输入合理正整数！" + "</p>";
					    } 
