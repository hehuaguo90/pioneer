			            $(this).val("");
			        });
		 		var inputs = newtr.find("input");
				$(inputs[1]).attr("id","pzmc"+index);
				$(inputs[2]).attr("id","ly"+index);
				$(inputs[3]).attr("id","cngz"+index);
				$(inputs[4]).attr("id","hbgz"+index);
				$(inputs[5]).attr("id","cdgz"+index);
				$(inputs[6]).attr("id","jyscl"+index);
				$(inputs[7]).attr("id","jyxsl"+index);
