						 i++;
						 var key = $(sls[j]).attr("id");
						 $("#" + key).addClass("error");
						   showMsg += '<p class="text-danger" onclick="locationErr(\'' + key + '\', this)">' + i + "、" + "数量" + "，" + "请输入合理正整数！" + "</p>";
					    } 
				 }
				 var nomsg = '<div class="nomsg"><span class="ico icon-coffee"></span>暂无验证信息！</div>';
				 if(showMsg){
					   $("#form_check_msg").html(showMsg);
					   $("#form_check_msg").show();
					   showErrMsg();
					   return false;
				   }else{
					   $("#form_check_msg").html(nomsg);
					   return true;
				   }
				   
			 }
				
