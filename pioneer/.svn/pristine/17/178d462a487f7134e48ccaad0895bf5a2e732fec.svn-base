@charset "utf-8";
/*顶部菜单栏样式开始*/


.pagetop .nav-pills > li.active > a, .nav-pills > li.active > a:focus, .nav-pills > li.active > a:hover,.pagetop .nav-pills > li.active > a:active {
	background-color: #43bd77;
	color: #fff;
}
.pagetop .nav > li > a {
	color: #d0d4d7;
}
.pagetop .nav > li > a:focus, .nav > li > a:hover {
	text-decoration: none;
	background-color: #33495c;
	color: #fff;
}



/*顶部菜单栏样式结束*/
.text-warning {
    color: #d48b44;
}
.text-danger {
    color: #f45555;
}
.text-danger:hover {
    color: #d39500;
}

.carousel-control.left {
	display:none;
  background-image: -webkit-linear-gradient(left, rgba(0, 0, 0, 0.5) 0%, rgba(0, 0, 0, 0.0001) 1%);
  background-image: -o-linear-gradient(left, rgba(0, 0, 0, 0.5) 0%, rgba(0, 0, 0, 0.0001) 1%);
  background-image: linear-gradient(to right, rgba(0, 0, 0, 0.5) 0%, rgba(0, 0, 0, 0.0001) 1%);
  background-repeat: repeat-x;
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#80000000', endColorstr='#00000000', GradientType=0);
}

.carousel-control.right {
	display:none;
  left: auto;
  right: 0;
  background-image: -webkit-linear-gradient(left, rgba(0, 0, 0, 0.0001) 0%, rgba(0, 0, 0, 0.5) 100%);
  background-image: -o-linear-gradient(left, rgba(0, 0, 0, 0.0001) 0%, rgba(0, 0, 0, 0.5) 100%);
  background-image: linear-gradient(to right, rgba(0, 0, 0, 0.0001) 0%, rgba(0, 0, 0, 0.5) 100%);
  background-repeat: repeat-x;
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#00000000', endColorstr='#80000000', GradientType=1);
}

.modal-title{
	color: #48A448;
	font-weight:bold;
}
.pagination>.active>a, .pagination>.active>a:focus, .pagination>.active>a:hover, .pagination>.active>span, .pagination>.active>span:focus, .pagination>.active>span:hover {
    z-index: 3;
    color: #fff;
    cursor: default;
    background-color: #5cb85c;
    border-color: #5cb85c;
}
a {
    color: #46A746;
    text-decoration: none;
}
.pagination>li>a, .pagination>li>span {
    position: relative;
    float: left;
    padding: 6px 12px;
    margin-left: -1px;
    line-height: 1.42857143;
    color: #46A746;
    text-decoration: none;
    background-color: #fff;
    border: 1px solid #ddd;
   
}
.nav-tabs > li > a{ border-radius: 0;}
.nav-tabs > li.active > a{border-top:2px solid #46A746;}
.nav-tabs > li > a:hover,.nav-tabs > li.active > a:hover ,.nav-tabs > li.active > a:focus{
    /*text-decoration: none;
    background-color: #449d44;
    color: #46A746;*/background-color:transparent;color: #46A746;border-top:2px solid #46A746;border-bottom:1px solid #fff;
}
a:focus, a:hover {
    color: #46A746;
    text-decoration: underline;
}
