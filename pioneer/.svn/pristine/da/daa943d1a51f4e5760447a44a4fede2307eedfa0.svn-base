<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%@taglib prefix="t" tagdir="/WEB-INF/tags"%>
<%@taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<%@taglib prefix="fn" uri="http://java.sun.com/jsp/jstl/functions"%>
<%@taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt"%>
<t:page>
	<jsp:attribute name="head">
<script type="text/javascript"
			src="${ contextPath }/res/sys/main/page.js"></script>


<script type="text/javascript">
    page.onLoad = function() {
                page.send({
                    url : contextPath + "/tCyNfzysyqk/getBeanById/?${pageContext.request.queryString}"
                }).then(function(data) {
                    //  item = kernel.json.parse(data);
                    page.loadForm({data:data });
                });

                //page.loadForm({url : contextPath + "/tCyNfzysyqk/getBeanById/?${pageContext.request.queryString}"});

	};
</script>

</jsp:attribute>
	<jsp:body>
	<form method="post" action="${ contextPath }/tCyNfzysyqk/edit">
        <table>
            <tr>
                <th>id:</th>
                <td>

                            <input id="id" name="id"  type="text"
                                   style="width: 146px;"   class="easyui-validatebox textbox"
                                   data-options="validType:'length[1,10]'" >

                </td>
            </tr>
            <tr>
                <th>填报编码:</th>
                <td>

                            <input id="tbbm" name="tbbm"  type="text"
                                   style="width: 146px;"   class="easyui-validatebox textbox"
                                   data-options="validType:'length[1,10]'" >

                </td>
            </tr>
            <tr>
                <th>农副资源类别:</th>
                <td>

                            <input id="nfzylb" name="nfzylb"  type="text"
                                   style="width: 146px;"   class="easyui-validatebox textbox"
                                   data-options="validType:'length[1,10]'" >

                </td>
            </tr>
            <tr>
                <th>产量:</th>
                <td>

                            <input id="cl" name="cl"  type="text"
                                   style="width: 146px;"   class="easyui-validatebox textbox"
                                   data-options="validType:'length[1,22]'" >

                </td>
            </tr>
            <tr>
                <th>饲用量:</th>
                <td>

                            <input id="syl" name="syl"  type="text"
                                   style="width: 146px;"   class="easyui-validatebox textbox"
                                   data-options="validType:'length[1,22]'" >

                </td>
            </tr>
            <tr>
                <th>加工饲用量:</th>
                <td>

                            <input id="jgsyl" name="jgsyl"  type="text"
