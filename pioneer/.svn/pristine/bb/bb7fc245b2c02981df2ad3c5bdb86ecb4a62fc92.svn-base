#left_form_turn{display:block;width:30px;height:30px;position:absolute;left:1px;top:1px;cursor:pointer;z-index:200;}
.tree-bar{height:30px;line-height:30px;font-size:14px;font-weight:bold;padding-left:10px;/*border-bottom:1px solid #cdcdcd;background-color:#e7f7fe;*/}
.no-form{color:#dedede;font-size:40px;line-height:300px; text-align:center;}



/*通用非EASYUI选项卡样式开始*/
.simple_tab {
	
	height: 32px;
	line-height: 32px;
	text-align: center;
	padding-right: 10px;
	padding-top:8px;
	
	border-bottom:1px solid #5fc0f3;
}
.simple_tab li {
	float:right;
	min-width:33px;
	
	white-space:nowrap;
	height: 31px;
	line-height: 32px;
	color: #717171;
	padding: 0px 15px 0px 15px;
	text-align: center;
	cursor: pointer;
	border:1px solid #CCC;
	border-bottom:none;
	margin-left:5px;
}
.simple_tab li.active {
	color: #119EEA;
	height: 31px;
	font-weight: bold;
	background-color: #fff;/*64a5cd*/
	border-radius: 0px;
	border: 1px solid #5fc0f3;
	border-bottom: 1px solid #fff;
}
/*通用非EASYUI选项卡样式结束*/


.chart_list_option_title{position:absolute;top:50px;left:8px;width:70px;height:25px;z-index:9999;text-align:center;font-size:12px;font-weight:bold;color:#333333;}
.chart_ico_list{width:75px;margin:0px;padding:0px;margin-top:0px;position:absolute;top:75px;left:8px;}
.chart_ico_list li{width:64px;margin-bottom:7px;border:1px solid #2dbff5;padding:8px 0;background-color:#f8f8f8;text-align:center;position: relative;min-height:65px;}
.chart_ico_list li .fa{font-size:33px;color:#04a6e2;}
.chart_ico_list li p{font-size:12px;font-family:"微软雅黑";width:54px;margin:auto;margin-top:0px;color:#353535;text-align:center;}
.chart_ico_list .operation{position:absolute;border:1px solid #2dbff5;width:250px;top:-1px;bottom:-1px;left:64px;display:none;background-color:#fff;}
.chart_ico_list li:hover .operation{display:block;}
.chart_ico_list .btnbox{padding:8px;text-align:left;font-size:12px;color:#666;font-family:"微软雅黑";}
.chart_ico_list .sliderbox{padding:10px;text-align:center;padding-left:14px;padding-top:14px;}
.chart_ico_list .slider-tip{color:#06C;/*background-color:#6FF; */border-radius: 5px;}
.chart_ico_list .sliderbox .slider-inner {
    border-color: #C2DCFF;
    background: #E0ECFF;
}
.chart_ico_list .sliderbox .slider-inner {
    position: relative;
    height: 6px;
    top: 9px;
    border-width: 1px;
    border-style: solid;
    border-radius: 5px;
}
