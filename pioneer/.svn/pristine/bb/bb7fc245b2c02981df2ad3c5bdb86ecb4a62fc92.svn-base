@CHARSET "UTF-8";
@import url(easyui.css);
#location {
	padding-left: 40px;
	background: url('images/title_ico.png') no-repeat 10px center;
}

#location a, #location span {
	color: #000000;
}

#location .current {
	font-size: 14px;
	color: #008017;
}


#searchbar {
	border: 1px solid #a8e2f8;
	padding: 10px;
	background-color: #fdfdfd;
	font-size: 12px;
}

form table th, form table td {
	border: 1px solid #ececec;
	font-size: 12px;
}

form table th {
	background-color: #e7f7fe;
	font-weight: normal;
	text-align: right;
	height:30px;
	padding-right:3px;
}

form table {
	background-color: #fff;
	border-collapse: collapse;
	empty-cells: show;
	width: 100%;
}

.formbox table th, .formbox table td {
	border: 1px solid #ececec;
	font-size: 12px;
}

.formbox table th {
	background-color: #e7f7fe;
	font-weight: normal;
	text-align: right;
	height:30px;
	padding-right:3px;
}

.formbox table {
	background-color: #fff;
	border-collapse: collapse;
	empty-cells: show;
	width: 100%;
}

.showcode{text-align:center!important;}
.showcode img{margin:6px;}
.loadpanel .imgDel {
width: 16px;
height: 16px;
background: url('images/wtg.png') no-repeat left top!important;
}
.step_bar {
	display:block;
	background-color: #FFF;
	border: 1px solid #d7d7d7;
	height:42px;
	padding:0px;
	margin:0px;
	margin-bottom:10px;
	
}
.step_bar li{
	padding:0px;
	text-align: center;
	float: left;
	height: 42px;
	line-height: 42px;
	list-style: none;
	width: 25%;
	background-image: url(images/stepbg.png);
	background-repeat: no-repeat;
	background-position: right center;
	font-family:"微软雅黑";
	font-size:14px;
	color:#4b4b4b;
	/*cursor:pointer;*/
}
.step_bar li.step_on{
	background-image: url(images/stepbg.png);
	background-repeat: no-repeat;
	background-position: right top;
	background-color:#17aa75;
	font-size:16px;
	color:#fff;
	font-weight:bold;
	
}
.step_bar li.laststep_off{
	background:none;
	background-color:#fff;
}
.step_bar li.laststep_on{
	background-image: none;
	background-color:#17aa75;
	font-size:16px;
	color:#fff;
	font-weight:bold;
	
}
.step_bar li.front_step{
	background-image: url(images/stepbg.png);
	background-repeat: no-repeat;
	background-position: right bottom;
}
.stepcontent{margin:auto;width:80%;}

.resetpassbox{
	width:649px;
	height:355px;
	margin:auto;
	background-image: url(images/passresetbg.png);
	background-repeat: no-repeat;
	background-position: center center;
	border:1px solid #ebebeb；
}
.resetpassbox td{}
.resetpasserror{
	width: 490px;
	height: 180px;
	margin: auto;
	margin-top: 100px;
	background-color: #fff;
	color: #515151;
	background-image: url(images/cryface.png);
	background-repeat: no-repeat;
	background-position: 40px center;
	}
.resetpasserror td{color:#7c7c7c;}
.errortitle{font-weight:bold;color:#ff6600!important;font-size:16px;}
.errorcontent a{color:#7c7c7c;line-height:22px;}
.errorcontent a:hover{color:#ff6600;}
.msgpaopao {font-size:12px;color:#F00;}
.tc_qymc{color:#F00;}
.tc_yjlx{color:#9f9f9f;}
.tc_yjsj{color:#9f9f9f;}
.tc_yjmc{color:#F00;}
.msgpaopao td{min-height:53px;border-bottom:1px  dotted #cdcdcd;}
.btn_orange {
	line-height:22px;
	height:25px;
	background: url('images/btn_orange_out.png') repeat-x left bottom;
	text-align:center;
	font-size:12px;
	font-family:"微软雅黑";
	color:#fff;
	padding-left:10px;
	padding-right:10px;
	display:inline-block;
	border:none;
	cursor:pointer;
	
}

.btn_orange:hover {
	height:25px;
	background: url('images/btn_orange_over.png') repeat-x left bottom;
}

ul,li{list-style:none;}
.industry_list{max-width:1366px;margin:auto;}
.industry_list li{height:60px;min-width:150px;float:left;text-align:left;margin:10px 20px 10px 3px;}
.industry_list li p{
	margin-left: 3px;
	display: inline-block;
	min-width: 180px;
	height: 60px;
	line-height: 60px;
	border: 1px solid #a8e2f8;
	background-color: #e7f7fe;
	font-size: 16px;
	font-family: "微软雅黑";
	padding: 0 15px 0 82px;
	cursor: pointer;
	background-image: url(images/files_ico.png);
	background-repeat: no-repeat;
	background-position: 6px center;
}
.industry_list li p:hover{background-color: #fff;}
.industry_list li span{padding:8px;font-size:14px;font-weight:bold;font-family:Arial, Helvetica, sans-serif;color:#006c95;display:none;}
.industry_list li input{display:none;}
.industry_list_box{
	background-image: url(images/hylist_bg.png);
	background-repeat: no-repeat;
	background-position: center bottom;
}

