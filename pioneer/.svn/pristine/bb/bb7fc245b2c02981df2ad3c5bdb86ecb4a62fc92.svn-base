.qanda{margin:20px;}
.qanda li{color:#333333;font-size:12px;margin-bottom:20px;}
.xuhao{width:40px;height:40px;line-height:40px;font-size:24px;color:#ff9900;float:left;font-family:"微软雅黑";}
