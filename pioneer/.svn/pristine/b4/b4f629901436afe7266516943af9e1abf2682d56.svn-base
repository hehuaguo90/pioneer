@charset "utf-8";
* {
	font-family: "微软雅黑", "Microsoft YaHei ";
}
@font-face {
	font-family: 'impact';   /*字体名称*/
	src: url('impact-webfont.woff');
}
body, html, div, p {
	padding: 0;
	margin: 0;
}
ul, li {
	list-style: none;
	padding: 0;
	margin: 0;
}
em{font-style:normal;}
.clear {
	clear: both;
}
a {
	/*初始化A的样式，不要下划线*/
	text-decoration: none!important;
	color: #343844;
	cursor:pointer;
}
.bodybg {
	/*主框架页面上方的背景白云图片*/
	background-color: #ecf0f5;
	background-image: url(images/pagebg_top.png);
	background-repeat: repeat-x;
	background-position: right top;
	margin: 0;
	padding: 0;
}
.bodybg_ldzm {
	/*领导桌面页面上方的背景白云图片*/
	background-color: #262a31;
	background-image: url(images/ldzm_bg.jpg);
	background-repeat: no-repeat;
	background-position: center top;
	margin: 0;
	padding: 0;
}
