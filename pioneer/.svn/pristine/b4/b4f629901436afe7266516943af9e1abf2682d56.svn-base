/*左侧边条样式*/
.bodybg_box {
	/*整个页面框架的外壳，包含了页面下方的背景，和BODY上的背景一起形成可自适应高度的背景*/
	
	background-image: url(images/pagebg_bottom.png);
	background-repeat: repeat-x;
	background-position: right bottom;
	position: relative;
	height: 100%;
	width: 100%;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
}
.chart_bodybg_box {
	/*包含了页面下方的背景，和BODY上的背景一起形成背景*/
	background-image: url(images/pagebg_bottom.png);
	background-repeat: repeat-x;
	background-position: right bottom;
	width: 100%;
	padding-bottom: 16px;
}
.chart_bodybg_box1 {
	/*包含了页面下方的背景，和BODY上的背景一起形成背景*/
	background-image: url(images/sysselectbg.jpg);
	background-size:100% 100%;
	width: 100%;
	padding-bottom: 16px;
}
