	      var createTextStyle = function(feature, text,xValue,yValue) {
	          var align = 'Center';
	          var baseline = 'Middle';
	          var size = '12px';
	          var offsetX = parseInt(-xValue, 10);
	          var offsetY = parseInt(yValue, 10);
	          var weight = 'bold';
	          var rotation = parseFloat('Arial');
