	          var outlineWidth = parseInt('8', 10);
			  var x=0;
			  var y=25;
	          var length = text.length;
	          if(length){
	        	  if(length == 1){
	        		  x = 3;
	        		  y = 20;
	        	  }else if(length == 2){
	        		  x = 6;
	        		  y = 20;
	        	  }else if(length == 3){
	        		  x=10;
	        		  y=20;
	        	  }
	          }
	          
