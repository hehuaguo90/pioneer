box-shadow: 0px 0px 10px #7ca9cd;}
/*菜单动画效果开始*/
.main_menu img:hover
{
animation:myfirst 0.2s;
-moz-animation:myfirst 0.2s; /* Firefox */
-webkit-animation:myfirst 0.2s; /* Safari and Chrome */
-o-animation:myfirst 0.2s; /* Opera */
}

@keyframes myfirst
{
from {position:relative;width:154px;top:0px;left:0px;z-index:100;}
to {position:relative;width:198px;top:-30px;left:-20px;z-index:1000;}
}

@-moz-keyframes myfirst /* Firefox */
{
from {position:relative;width:154px;top:0px;left:0px;z-index:100;}
to {position:relative;width:198px;top:-30px;left:-20px;z-index:1000;}
}

@-webkit-keyframes myfirst /* Safari and Chrome */
{
from {position:relative;width:154px;top:0px;left:0px;z-index:100;}
to {position:relative;width:198px;top:-30px;left:-20px;z-index:1000;}
}

@-o-keyframes myfirst /* Opera */
{
from {position:relative;width:154px;top:0px;left:0px;z-index:100;}
to {position:relative;width:198px;top:-30px;left:-20px;z-index:1000;}
}

/*菜单动画效果结束*/




