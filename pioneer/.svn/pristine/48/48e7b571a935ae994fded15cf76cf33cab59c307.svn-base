@CHARSET "UTF-8";

.panel-title {
	font-size: 14px;
	font-weight: bold;
	color: #fff;
	height: 16px;
	line-height: 16px;
	margin: 5px 0px 0px 5px;
}

.window {
	padding: 0px;
}

.window .window-header .panel-tool {
	right: 5px;
}

.window-shadow {
	background: transparent;
	-moz-box-shadow: none;
	-webkit-box-shadow: none;
	box-shadow: none;
	filter: none;
}

.window .window-body {
	border-color: #6dc3a4;
}

.window {
	background-color: #23ab76;
	background: -webkit-linear-gradient(top, #23ab76 0, #23ab76 20%);
	background: -moz-linear-gradient(top, #23ab76 0, #23ab76 20%);
	background: -o-linear-gradient(top, #23ab76 0, #23ab76 20%);
	background: linear-gradient(to bottom, #23ab76 0, #23ab76 20%);
	background-repeat: repeat-x;
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#23ab76,
		endColorstr=#23ab76, GradientType=0);
}

#searchbar .l-btn {
	background: url('images/btn_green_out.png') !important;
	color: #fff;
	border: none;
	padding: 0 4px 0 4px;
	height: 24px;
	line-height: 24px;
	-moz-border-radius: 0px 0px 0px 0px;
	-webkit-border-radius: 0px 0px 0px 0px;
	border-radius: 0px 0px 0px 0px;
}

#searchbar .l-btn:hover {
	background: url('images/btn_green_over.png') !important;
	color: #fff;
	border: none;
}

#operator .l-btn {
	background: url('images/btn_green_out.png') !important;
	color: #fff;
	border: none;
	padding: 0 4px 0 4px;
	height: 24px;
	line-height: 24px;
	-moz-border-radius: 0px 0px 0px 0px;
	-webkit-border-radius: 0px 0px 0px 0px;
	border-radius: 0px 0px 0px 0px;
}

#operator .l-btn:hover {
	background: url('images/btn_green_over.png') !important;
	color: #fff;
	border: none;
}

.textbox {
	-moz-border-radius: 0px 0px 0px 0px;
	-webkit-border-radius: 0px 0px 0px 0px;
	border-radius: 0px 0px 0px 0px;
}

.tabs li a.tabs-inner {
	-moz-border-radius: 0 0 0 0;
	-webkit-border-radius: 0 0 0 0;
	border-radius: 0 0 0 0;
}

.tabs li.tabs-selected a.tabs-inner {
	color: #ffffff;
	background-color: #e1e1e1;
	background: url('images/tab_on.png') left bottom;
	border-top: 1px;
	filter: none;
}

.tabs li.tabs-selected a.tabs-close {
	background: url('images/tabs_icons.png') no-repeat -47px center;
}

.tabs li a.tabs-inner {
	color: #008017;
	background: url('images/tab_bg.png') left bottom;
	background-color: #e1e1e1;
	border-top: 2px solid #07ac8f;
}

.tabs li a.tabs-inner {
	height: 24px !important;
}

.tabs-header-plain {
	background: transparent;
}

.tabs-header, .tabs-scroller-left, .tabs-scroller-right, .tabs-tool,
	.tabs, .tabs-panels, .tabs li a.tabs-inner, .tabs li.tabs-selected a.tabs-inner,
	.tabs-header-bottom .tabs li.tabs-selected a.tabs-inner,
	.tabs-header-left .tabs li.tabs-selected a.tabs-inner,
	.tabs-header-right .tabs li.tabs-selected a.tabs-inner {
	border-color: #07ac8f;
}

.tabs li.tabs-selected a.tabs-inner {
	border-bottom: none;
	height: 26px !important;
}

.messager-button .l-btn {
	background: url('images/btn_green_out.png') !important;
	color: #fff;
	border: none;
	padding: 0 4px 0 4px;
	height: 24px;
	line-height: 24px;
	-moz-border-radius: 0px 0px 0px 0px;
	-webkit-border-radius: 0px 0px 0px 0px;
	border-radius: 0px 0px 0px 0px;
}

.messager-button .l-btn:hover {
	background: url('images/btn_green_over.png') !important;
	color: #fff;
	border: none;
}

.tabs-header, .tabs-tool {
	background-color: #dff1e7 !important;
}

.tabs li a.tabs-close {
	background: rgba(0, 0, 0, 0) url("images/tabs_icons.png") no-repeat
		scroll -34px center !important;
}

.window-proxy-mask, .window-mask {
	background: #444444 none repeat scroll 0 0;
}

.window-shadow {
	display: none !important;
}

.datagrid-header td, .datagrid-body td, .datagrid-footer td {
	border-style: solid;
	border-width: 0 1px 1px 0;
	margin: 0;
	padding: 0;
	border-color: #8ddea8;
	color: #000;
	height: 30px;
}

.datagrid-header {
	background: none;
	filter: none;
	background-color: #d8f9e3;
}

.datagrid-row-alt {
	background: #f1f1f1 none repeat scroll 0 0;
}

.panel-header, .panel-body {
	border-color: #cdcdcd;
}

.datagrid-row-over {
	background: #effff3;
	color: #000000;
	cursor: default;
}

.datagrid-row-selected {
	background: #8ddea8 none repeat scroll 0 0;
	color: #000000;
}

.window, .window .window-body {
	border-color: #6dc3a4;
}

.datagrid-header td.datagrid-header-over {
	background: #effff3;
	color: #000000;
	cursor: default;
}

.datagrid-td-rownumber {
	background: none;
}

.datagrid-sort-asc .datagrid-sort-icon {
	background: rgba(0, 0, 0, 0) url("images/datagrid_icons.png") no-repeat
		scroll 0 center;
	display: inline;
	padding: 0 13px 0 0;
}

.datagrid-sort-desc .datagrid-sort-icon {
	background: rgba(0, 0, 0, 0) url("images/datagrid_icons.png") no-repeat
		scroll -16px center;
	display: inline;
	padding: 0 13px 0 0;
}

.tabs-scroller-left {
	background: #f6fffa url("images/tabs_icons.png") no-repeat scroll 1px
		center;
}

.tabs-scroller-right {
	background: #f6fffa url("images/tabs_icons.png") no-repeat scroll -15px
		center;
}

.tabs-scroller-right {
	right: 0;
}

.tabs li a.tabs-inner:hover {
	filter: none;
}

.tabs li.tabs-selected a.tabs-close {
	background: url('images/tabs_icons.png') no-repeat -47px center
		!important;
}

.panel-header {
	background-color: #F4F4F4;
	background: -webkit-linear-gradient(top, #F4F4F4 0, #F4F4F4 100%);
	background: -moz-linear-gradient(top, #F4F4F4 0, #F4F4F4 100%);
	background: -o-linear-gradient(top, #F4F4F4 0, #F4F4F4 100%);
	background: linear-gradient(to bottom, #F4F4F4 0, #F4F4F4 100%);
	background-repeat: repeat-x;
	filter: none;
}

.datagrid .panel-header .panel-title {
	color: #000;
}

.window .panel-header {
	filter: none;
}

.window, .window-shadow {
	border-radius: 0px;
}

.l-btn {
	filter: none;
}

#searchbar .easyui-linkbutton[iconCls="icon-search"], #operator .easyui-linkbutton:first-child,
	.messager-button .l-btn:first-child {
	background: url('images/btn_orange_out.png') repeat-x left bottom
		!important;
}

#searchbar .easyui-linkbutton[iconCls="icon-search"]:hover, #operator .easyui-linkbutton:first-child:hover,
	.messager-button .l-btn:first-child:hover {
	background: url('images/btn_orange_over.png') repeat-x left bottom
		!important;
}

.datagrid-htable {
	color: #006331;
	border-collapse: separate;
}

.datagrid-htable div.datagrid-cell {
	text-align: center !important;
}

.datagrid-htable div.datagrid-cell,.datagrid-htable div.datagrid-cell span,
	.datagrid-htable div.datagrid-cell span a {
	display: block; 
	height : 30px;
	line-height: 30px;
	
}
.datagrid-htable{height:31px!important;}
.datagrid-cell-rownumber {
	color: #a7a7a7;
}

.datagrid td a, .datagrid th a {
	color: #3d9e5d !important;
}

.datagrid {
	margin: auto;
}

.datagrid-header-row span a {
	text-decoration: none !important;
	font-weight: bold;
}

.td-select {
	background: #FBEC88
} /*td高亮样式*/