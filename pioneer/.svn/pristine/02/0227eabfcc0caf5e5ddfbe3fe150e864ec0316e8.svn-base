}
.tree-icon{vertical-align:middle;}
.tree-expanded, .tree-collapsed, .tree-folder, .tree-file, .tree-checkbox, .tree-indent {
    display: inline-block;
    width: 16px;
    height: 18px;
	margin-top:4px;
    vertical-align: top;
    overflow: hidden;
}
.tree-node-selected {
	color: #000000;
	background-color: #A6DBFF;
	border-radius:3px;
}
