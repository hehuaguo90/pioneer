/*数据展现界面样式区结束*/
.form_add_box{padding:20px;text-align:center;}
.form_closebox{position:relative;}
.form_closebox .closebtn{
    display: none;
    width: 20px;
    height: 37px;
    font-size: 20px;
    position: absolute;
    top: 4px;
    right: -12px;
    cursor: pointer;
    color: rgb(209, 114, 114);
}
.form_closebox:hover .closebtn{display: block;}
.tkform table{width:90%;margin:auto;}
.tkform th,.tkform td{font-weight:normal;padding:3px;}
